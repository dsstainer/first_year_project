{
	"name": "first-group-project-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"testbuildandprod": "npm run testbuild && npm run prod",
		"properbuildandprod": "npm run properbuild && npm run prod",
		"testbuild": "vite build && tsc --project tsconfig.prod.json && find ./dist -type f -name \"*.js\" -exec sed -i 's|./build/handler.js|./../build/handler.js|g' {} +",
		"properbuild": "vite build && tsc --project tsconfig.prod.json && cp -r build dist",
		"prod": "node dist/server.js",
		"_comment": "\"preview\": \"vite preview\"",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.4",
		"@types/cookie": "^0.5.1",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"p5-svelte": "^3.1.2",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-pocketbase": "^0.0.4",
		"tslib": "^2.4.1",
		"typescript": "^5.3.2",
		"vite": "^4.4.2",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"pocketbase": "^0.19.0",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"ws": "^8.14.2"
	}
}
